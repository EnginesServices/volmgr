{
  "title": "volmgr",
  "description": "Volume Manager",
  "memory_usage": {
    "required": 32,
    "recommended": 32
  },
  "licenses": [
  ],
  "binding_anchor": {
    "variables": {
      "volume": "^^identifier^^",
      "path": "/home/fs/^^identifier^^",
      "permissions": "rw",
      "user": "^^user.data_uid^^",
      "group": "^^user.data_gid^^",
      "fw_user": "^^user.identifier^^",
      "access_level": "6666",
      "exclude_from_backup": "false",
    }
  },
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "state": {
        "type_path": "filesystem/local/filesystem",
        "service_handle_field": "service_name",
        "deployment_type": "worker",
        "set_state": "running",
        "default_stop_timeout": 0,
        "restart_policy": "unless-stopped",
        "restart_attempts": 0,
        "run_as_user": "volmgr",
        "create_user": "1"
      },
      "disposition": [
        "persistent",
        "attach_post_build",
        "attach_requires_restart",
        "shareable",
        "consumer_exportable"
      ],
      "constants": [
        {
          "name": "backup_support",
          "value": "true"
        }
      ],
      "environment": {
        "variables": [
          {
            "name": "engine_path",
            "value": "VOLDIR"
          },
          {
            "name": "service_name",
            "value": "volume_name"
          }
        ]
      }
    }
  ]
}
