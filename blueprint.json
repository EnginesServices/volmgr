{
  "title": "volmgr",
  "description": "Volume Manager",
  "memory_usage": 32,
  "licenses": [
  ],
  "binding_anchor": {
    "variables": {
      "volume": "^^identifier^^",
      "path": "/home/fs/^^identifier^^",
      "permissions": "rw",
      "user": "1111",
      "group": "1111",
      "container_user_identifier": "^^user.identifier^^",
      "access_level": "6666",
      "exclude_from_backup": "false"
    }
  },
  "user": {
    "identifier": "22007",
    "name": "volmgr",
    "create": true
  },
  "stages": [
    {
      "starter": {
        "image": "engines/servicebase",
        "tag": "current"
      },
      "environment": {
        "variables": [
          {
            "name": "engine_path",
            "value": "VOLDIR"
          },
          {
            "name": "service_name",
            "value": "volume_name"
          },
          {
            "name": "backup_support",
            "value": "true"
          }
        ]
      }
    }
  ]
}
